{
  "presets": [
      [
        "@babel/preset-env",
        {
          "targets": { "node": "current" }
        }
      ],
      "@babel/preset-react"
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "regenerator": true
      }
    ]
  ],
  "env": {
    "production": {
      "plugins": ["@babel/plugin-syntax-dynamic-import"]
    }
  }
}